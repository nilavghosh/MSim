<section data-ng-app="ngHandsontableDemo" class="AdminView">
    <div data-ng-controller="DemoCtrl">
        <div class="row">
            <h3>FMCG - Admin - Algo Manage</h3>
        </div>
        <div class="row">
            <div class="cells12">


                <hot-table id="example2"
                           colheaders="true"
                           datarows="data1"></hot-table>


                <h2>Data binding</h2>

                <p>The below <code>input</code> field is bound to the <code>address</code> property of the first data row.</p>

                <label>Address <input type="text" data-ng-model="data1[1][3]" style="width: 300px"></label>
            </div>
        </div>
        <div class="row">
            <div class="cells12">
                <button class="btn btn-default" ng-click="saveAdminSheet()" type="submit">Save</button>
            </div>
        </div>
    </div>





</section>

@Scripts.Render("~/bundles/admin")
