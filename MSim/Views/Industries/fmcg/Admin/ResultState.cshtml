<section id="fmcg-admin" class="view">
    <!-- ko with: fmcg -->
    <div class="grid">
        <div class="row">
            <h3>FMCG - Admin - Algo Manage</h3>
        </div>
        <div class="col-md-4">
            <div class="row">
                <form class="form-horizontal" action="#/Save" method="post">
                    <div class="form-group">
                        <label>PTD</label>
                        <input type="text" data-bind="value: FMCGData.PTD" class="form-control" id="inputEmail3">

                    </div>
                    <div class="form-group">
                        <label>Distributor Margin</label>
                        <input type="number" data-bind="value: FMCGData.DistributorMargin" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label>Retailer Margin</label>
                        <input type="number" data-bind="value: FMCGData.RetailerMargin" class="form-control" />
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-default">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- /ko -->
</section>
